{
  "name": "@cortex-ai/sdk",
  "description": "Cortex SDK for Node.js",
  "version": "0.6.8",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "build": "bunup --dts.resolve --exports --target browser",
    "dev": "pnpm build --watch",
    "dev:hono": "pnpm -C examples/hono dev",
    "test": "vitest",
    "generate": "bun src/generate/index.ts && prettier --write src/openapi.ts",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "fresh-deps": "pnpx taze -w",
    "release": "bumpp",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/debug": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "bumpp": "^10.1.0",
    "bunup": "^0.14.20",
    "chalk": "^5.4.1",
    "debug": "^4.4.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^7.6.1",
    "openapi-typescript-helpers": "^0.0.15",
    "ora": "^8.2.0",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "cortex",
    "ai",
    "openai",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trycortexai/cortex-node.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eventsource-parser": "^3.0.1"
  }
}
